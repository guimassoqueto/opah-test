{
  "name": "opah-test",
  "version": "1.0.0",
  "description": "",
  "main": ".eslintrc.js",
  "scripts": {
    "build": "tsc",
    "tsc:watch": "tsc --watch",
    "node:watch": "node --watch dist/main/main.js",
    "start:ts": "ts-node src/main/main.ts",
    "lint:staged": "lint-staged",
    "test": "jest --passWithNoTests --runInBand",
    "test:simplified": "npm test -- --silent --noStackTrace --coverage=false --detectOpenHandles",
    "test:unit": "npm test -- --noStackTrace --silent --detectOpenHandles -c jest-unit.config.ts",
    "test:integration": "npm test -- --noStackTrace --silent --detectOpenHandles -c jest-integration.config.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.3",
    "@types/pg": "^8.10.1",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fast-glob": "^3.2.12",
    "pg": "^8.11.0",
    "winston": "^3.8.2"
  }
}
